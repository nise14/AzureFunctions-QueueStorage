{
  "version": "2.0",
  "extensions": {
    "queues": {
      "maxDequeueCount": 2
    }
  },
  "logging": {
    "fileLoggingMode": "always",
    "logLevel": {
      "WeatherDataIngestor": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "concurrency": {
    "dynamicConcurrencyEnabled": true,
    "snapshotPersistenceEnabled": true
  }
}